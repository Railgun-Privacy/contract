{
  "name": "railgun-contracts",
  "version": "0.0.1",
  "description": "Railgun Contract",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "solhint 'contracts/**/*.sol'; prettier --check './**'; eslint .",
    "lint:fix": "npx prettier --write './**'; npx solhint --fix 'contracts/**/*.sol'",
    "clean": "hardhat clean && rm -rf cache storage-layouts",
    "compile": "npm run clean && hardhat compile",
    "node": "hardhat node",
    "deploy": "hardhat deploy:test --network localhost",
    "test": "hardhat test --skip-long-tests",
    "test:complete": "hardhat test",
    "analyze": "npm run clean; hardhat compile; npm run lint; ./slither.sh"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
